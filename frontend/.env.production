# Production Environment Configuration
NODE_ENV=production

# API Configuration
VITE_API_URL=https://api.chefia-pos.com
VITE_WS_URL=wss://api.chefia-pos.com/ws

# App Configuration
VITE_APP_NAME=ChefIA POS System
VITE_APP_VERSION=1.0.0
VITE_DEBUG=false

# Feature Flags
VITE_ENABLE_DEBUG_TOOLBAR=false
VITE_ENABLE_HOT_RELOAD=false
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_ANALYTICS=true

# Authentication
VITE_JWT_EXPIRATION_HOURS=4
VITE_REFRESH_TOKEN_EXPIRATION_DAYS=3

# External Services
VITE_GOOGLE_MAPS_API_KEY=${GOOGLE_MAPS_API_KEY}
VITE_PAYMENT_GATEWAY_ENV=production

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_COMPRESSION=true