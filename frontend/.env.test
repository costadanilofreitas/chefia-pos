# Test Environment Configuration
NODE_ENV=test

# API Configuration
VITE_API_URL=http://localhost:8002
VITE_WS_URL=ws://localhost:8002/ws

# App Configuration
VITE_APP_NAME=ChefIA POS System (Test)
VITE_APP_VERSION=1.0.0-test
VITE_DEBUG=true

# Feature Flags
VITE_ENABLE_DEBUG_TOOLBAR=false
VITE_ENABLE_HOT_RELOAD=false
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_ANALYTICS=false

# Authentication
VITE_JWT_EXPIRATION_HOURS=1
VITE_REFRESH_TOKEN_EXPIRATION_DAYS=1

# External Services
VITE_GOOGLE_MAPS_API_KEY=test-google-maps-key
VITE_PAYMENT_GATEWAY_ENV=mock

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false

# Performance
VITE_ENABLE_PWA=false
VITE_ENABLE_SERVICE_WORKER=false

# Testing
VITE_TEST_TIMEOUT=10000
VITE_MOCK_API_DELAY=100